    "library",
    "es6",
    "videos",
    "images",
    "assets",
    "load"
  ],
  "unpkg": "dist/preload-it.min.js",
  "main": "dist/preload-it.js",
  "module": "dist/preload-it.esm.js",
  "module_minified": "dist/preload-it.esm.min.js",
  "browser": "dist/preload-it.js",
  "minified": "dist/preload-it.min.js",
  "dependencies": {},
  "devDependencies": {
